# Frontend (Vite) — copie para .env e preencha
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# (Opcional) flags locais de dev
SUPABASE_LOCAL_URL=
SUPABASE_ANON_KEY=
SUPABASE_LOCAL_SERVICE_ROLE_KEY=

# OpenAI (se precisar no frontend ou scripts locais)
OPENAI_API_KEY=
OPENAI_REALTIME_MODEL=gpt-4o-realtime-preview-2024-12-17
OPENAI_REALTIME_VOICE=verse

# Solana Configuration
# Cluster: devnet | mainnet-beta
VITE_SOLANA_CLUSTER=devnet
# Custom RPC (optional, defaults to public cluster URL)
# Devnet: https://api.devnet.solana.com
# Testnet: https://api.testnet.solana.com (menos usado)
# Mainnet: https://api.mainnet-beta.solana.com (PRODUÇÃO!)
VITE_SOLANA_RPC_URL=https://api.devnet.solana.com
# Merchant wallet address (recipient of payments)
VITE_MERCHANT_RECIPIENT=
# BRZ Token Mints (Brazilian Real stablecoin)
VITE_BRZ_MINT_DEVNET=
VITE_BRZ_MINT_MAINNET=

# Transfero (opcional)
TRANSFERO_BASE_URL=
TRANSFERO_CLIENT_ID=
TRANSFERO_CLIENT_SECRET=
TRANSFERO_ACCOUNT_ID=

# Webhook (opcional)
WEBHOOK_SECRET=

# Dev
DEMO_MODE=true
